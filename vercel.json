{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/widget.js",
      "dest": "/public/widget.js"
    },
    {
      "src": "/demo.html",
      "dest": "/public/demo.html"
    },
    {
      "src": "/landing.html",
      "dest": "/public/landing.html"
    },
    {
      "src": "/logos/(.*)",
      "dest": "/public/logos/$1"
    },
    {
      "src": "/screenshots/(.*)",
      "dest": "/public/screenshots/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
